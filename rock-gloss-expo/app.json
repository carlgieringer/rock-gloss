{
  "expo": {
    "name": "RockGloss",
    "description": "Rock climbing glossary",
    "slug": "rock-gloss",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "version": "1.0.4",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/carlgieringer/rock-gloss",
    "orientation": "portrait",
    "primaryColor": "#2f6888",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2f6888"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carlgieringer.rock-gloss",
      "buildNumber": "5",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.carlgieringer.rock_gloss",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-android-foreground.png",
        "backgroundColor": "#2f6888"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "rockgloss",
            "project": "rockgloss"
          }
        }
      ]
    }
  }
}