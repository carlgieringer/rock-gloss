{
  "expo": {
    "name": "RockGloss",
    "description": "Rock climbing glossary",
    "slug": "rock-gloss",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "version": "1.0.4",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/carlgieringer/rock-gloss",
    "orientation": "portrait",
    "primaryColor": "#2f6888",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2f6888"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carlgieringer.rock-gloss",
      "buildNumber": "4",
      "config": {
        "usesNonExemptEncryption": false
      }
    }
  }
}